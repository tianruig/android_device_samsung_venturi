# mount point	fstype		device
/efs		vfat		/dev/block/mmcblk0p10
/system		ext4		/dev/block/mmcblk0p13
/system_app	ext4		/dev/block/mmcblk0p14
/cache		ext4		/dev/block/mmcblk0p15
/data		ext4		/dev/block/mmcblk0p16

/emmc		vfat		/dev/block/mmcblk0p17 lun=/sys/devices/platform/usb_mass_storage/lun0/file
/sdcard		vfat		/dev/block/mmcblk1p1  lun=/sys/devices/platform/usb_mass_storage/lun1/file

/boot		emmc		/dev/block/mmcblk0p11
/recovery	emmc		/dev/block/mmcblk0p12
